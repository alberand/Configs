#!/bin/bash

# This script is used for connect to wiriless network

# Wi-fi interface for your wireless card. Can be found by this command:
# ip link
interface=wlp0s20u6
# Passfile is generated by this command:
# wpa_passphrase "NETWORK_NAME" "NETWORK_PASS" > file
passfile=./pass

sudo wpa_supplicant -D n180211,wext -i $interface -c $passfile &
dhcpcd $interface #&
